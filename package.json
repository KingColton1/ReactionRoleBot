    "lodash": "^4.17.20",
    "minimist": "^1.2.6",
    "multimap": "^1.1.0",
    "node-cache": "^5.1.2",
    "sqlite3": "^4.2.0",
    "ts-node": "^10.9.1",
    "winston": "^3.3.3"
  },
  "scripts": {
    "start": "NODE_ENV=prod npx ts-node .",
    "dev": "npx ts-node --logError . dev-config.json",
